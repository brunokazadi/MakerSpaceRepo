<% status = @user.return_program_status %>

<% if status[:volunteer] %>
  <%= render 'already_volunteer' %>
<% else %>
  <%= render 'congratulations_volunteer' %>
<% end %>
<hr />

<% if status[:dev] %>
  <%= render 'already_dev_program' %>
<% else %>
  <%= render 'congratulations_dev_program' %>
<% end %>
<hr />

<% if status[:teams] %>
  <%= render 'already_teams_program' %>
<% else %>
  <%= render 'congratulations_teams_program' %>
<% end %>
