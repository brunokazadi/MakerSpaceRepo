<!DOCTYPE html>
<html>
<head>
  <title>Admin Area | <%= page_title(yield(:title)) %></title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <%= favicon_link_tag 'site-icon.jpg' %>
    <%= stylesheet_link_tag    "vendor",      :media => "all", :debug => false %>
    <%= javascript_include_tag "vendor",      :debug => false %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= csrf_meta_tags %>
  <link href='http://fonts.googleapis.com/css?family=Raleway:700,300,400' rel='stylesheet' type='text/css'>
</head>
<body>
	<%= render 'layouts/header' %>

  <%= render :partial => 'partials/announcements' %>

  <%= render 'layouts/admin_area_header' %>

  <section>
    <div class="bg-white">
      <div class="container">
        <%= yield %>
      </div>
    </div>
  </section>

  <%= render 'layouts/footer' %>
</body>
</html>
