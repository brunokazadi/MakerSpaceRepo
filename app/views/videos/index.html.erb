<div class="container p-5">
  <div class="row text-center">
    <% @videos.each do |f| %>
      <div class="col-md-4" style="margin-bottom: 100px">
        <h5><%= f.proficient_project.title %></h5>
        <h5><%= f.processed ? "<h5 class='text-success'>Done<h5>".html_safe : "<h5 class='text-danger'>In progress...<h5>".html_safe  %></h5>
        <video autobuffer="autobuffer" preload="auto" controls="controls" width="100%" height="100%" >
          <source  src="<%= f.video.url%>"  />
        </video>
      </div>
    <% end %>
  </div>
</div>